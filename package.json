{
  "name": "@abernardobr/mynd-echarts",
  "version": "1.0.0",
  "description": "A full-featured Vue 3 wrapper for Apache ECharts with TypeScript support",
  "type": "module",
  "main": "./dist/mynd-echarts.cjs",
  "module": "./dist/mynd-echarts.js",
  "unpkg": "./dist/mynd-echarts.umd.js",
  "jsdelivr": "./dist/mynd-echarts.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mynd-echarts.js",
      "require": "./dist/mynd-echarts.cjs",
      "default": "./dist/mynd-echarts.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:examples": "vite --config vite.examples.config.ts",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "vite build --mode lib",
    "build:lib:check": "vue-tsc --noEmit && vite build --mode lib",
    "build:demo": "DEMO_BUILD=true vite build",
    "build:examples": "vite build --config vite.examples.config.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview",
    "preview:demo": "vite preview --outDir dist-demo",
    "preview:examples": "vite preview --config vite.examples.config.ts",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:watch": "vitest watch",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "echarts": "6"
  },
  "devDependencies": {
    "@monaco-editor/loader": "^1.5.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/node": "^20.14.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.0",
    "monaco-editor": "^0.52.2",
    "prettier": "^3.3.3",
    "terser": "^5.43.1",
    "typescript": "~5.5.0",
    "vite": "^5.3.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue": "^3.4.0",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abernardobr/mynd-echarts.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "echarts",
    "charts",
    "data-visualization",
    "typescript",
    "component",
    "wrapper"
  ],
  "author": "Alberto Bernardo <alberto@myndware.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abernardobr/mynd-echarts/issues"
  },
  "homepage": "https://github.com/abernardobr/mynd-echarts#readme"
}
